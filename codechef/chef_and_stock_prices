#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int s,a,b,c;
	    cin>>s>>a>>b>>c;
	    double x = (double)c/100 + 1;
	    s *= x;
	    if(s >= a && s <= b){
	        cout << "Yes" << endl;
	    }
	    else{
	        cout << "No" << endl;
	    }
	}
	return 0;
}
